        #getBC().setQueryFilter(new java.util.HashMap())
        #getBC().refresh(1)

        ret= ng1!.doModal()
        
        v! = #getBC().getSelection().clone()
         
        if ret then
                
        else
                #getBC().setSelection(v!)
        fi

    methodend

